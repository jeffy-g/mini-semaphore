{
  "name": "mini-semaphore",
  "version": "1.3.10",
  "description": "A lightweight version of Semaphore",
  "private": false,
  "main": "./cjs/index.js",
  "module": "./esm/index.mjs",
  "unpkg": "./umd/index.js",
  "sideEffects": false,
  "types": "./index.d.ts",
  "author": "jeffy-g",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jeffy-g/mini-semaphore/issues"
  },
  "defs": {
    "regex": "!function\\s*\\((.)\\)([^]+)(?=\\(t.restrictor\\s*\\|\\|)"
  },
  "homepage": "https://github.com/jeffy-g/mini-semaphore#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jeffy-g/mini-semaphore.git"
  },
  "engines": {
    "node": ">=v12.22.10",
    "yarn": "^1.22.4"
  },
  "files": [
    "package.json",
    "README.md",
    "LICENSE",
    "cjs",
    "esm",
    "umd",
    "webpack",
    "webpack-esm",
    "*.d.ts"
  ],
  "keywords": [
    "async",
    "lock",
    "mutex",
    "promise",
    "semaphore",
    "concurrency",
    "javascript",
    "typescript"
  ],
  "scripts": {
    "all": "yarn clean && yarn lint && yarn dist && yarn fire:tool copytypes",
    "build": "yarn fire:tool build",
    "clean": "rimraf build/* dist/* logs/*.tsbuildinfo",
    "dist": "yarn build && yarn fire:tool distExtra",
    "fire:tool": "bash scripts/tool.sh",
    "ic": "rimraf yarn.lock node_modules && yarn install",
    "up": "yarn upgrade-interactive --latest",
    "lint": "eslint src/**/*.ts",
    "pretest": "rimraf ./coverage/*",
    "test": "bash -c \"NODE_OPTIONS=--experimental-vm-modules npx jest --coverage --silent=false -c=jest.config.js\""
  },
  "devDependencies": {
    "@jeffy-g/cpx": "^1.6.4",
    "@types/jest": "^27.5.1",
    "@types/webpack": "^5.28.0",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "concurrently": "^7.2.0",
    "eslint": "^8.15.0",
    "jest": "^28.1.0",
    "node-fetch": "3.2.5",
    "organize-pkg": "^1.2.4",
    "rimraf": "^3.0.2",
    "terser-webpack-plugin": "^5.3.1",
    "ts-jest": "^28.0.2",
    "ts-loader": "^9.3.0",
    "typescript": "^4.6.4",
    "webpack": "^5.72.1",
    "webpack-cli": "^4.9.2"
  }
}
