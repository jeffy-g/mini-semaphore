{
  "name": "mini-semaphore",
  "version": "1.3.8",
  "description": "A lightweight version of Semaphore",
  "private": false,
  "main": "./cjs/index.js",
  "module": "./esm/index.js",
  "unpkg": "./umd/index.js",
  "sideEffects": false,
  "types": "./index.d.ts",
  "typings": "./index.d.ts",
  "author": "jeffy-g",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jeffy-g/mini-semaphore/issues"
  },
  "defs": {
    "regex": "/,\\s?\\w\\.t\\s?=[^]+(?=,\\s?i\\.o)/"
  },
  "homepage": "https://github.com/jeffy-g/mini-semaphore#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jeffy-g/mini-semaphore.git"
  },
  "engines": {
    "node": ">=v12.22.10",
    "yarn": "^1.22.4"
  },
  "files": [
    "package.json",
    "README.md",
    "LICENSE",
    "cjs",
    "esm",
    "umd",
    "webpack",
    "*.d.ts"
  ],
  "keywords": [
    "lock",
    "mutex",
    "promise",
    "semaphore",
    "concurrency",
    "javascript",
    "typescript"
  ],
  "scripts": {
    "all": "yarn clean && yarn lint && yarn dist && yarn webpack && yarn copy:types",
    "build": "concurrently -n tsc,build:esm -c blue,yellow \"tsc\" \"yarn build:esm\"",
    "build:esm": "tsc -p src/tsconfig.json",
    "clean": "rimraf build/* dist/* logs/*.tsbuildinfo",
    "copy:dts": "cpx -v src/index.d.ts",
    "copy:types": "concurrently  \"yarn copy:dts dist\" \"yarn copy:dts dist/cjs\" \"yarn copy:dts dist/esm\" \"yarn copy:dts dist/umd\" \"yarn copy:dts dist/webpack\"",
    "dist": "rimraf \"dist/!(webpack)\" && yarn build && cpx -v \"./{README.md,LICENSE}\" dist && cpx -v \"./build/**/*.js\" dist && orgpkg -p -k defs, -o dist",
    "ic": "rimraf yarn.lock node_modules && yarn install",
    "up": "yarn upgrade-interactive --latest",
    "lint": "eslint src/**/*.ts",
    "test": "bash -c \"NODE_OPTIONS=--experimental-vm-modules npx jest --coverage --silent=false -c=jest.config.js\"",
    "webpack": "rimraf ./dist/webpack/* ./dist/umd/* && npx webpack && cpx -v \"./dist/*.d.ts\" ./dist/webpack/"
  },
  "devDependencies": {
    "@jeffy-g/cpx": "^1.6.4",
    "@types/jest": "^27.4.1",
    "@types/webpack": "^5.28.0",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "concurrently": "^7.1.0",
    "eslint": "^8.14.0",
    "jest": "^28.0.0",
    "node-fetch": "3.2.3",
    "organize-pkg": "^1.2.3",
    "rimraf": "^3.0.2",
    "terser-webpack-plugin": "^5.3.1",
    "ts-jest": "^27.1.4",
    "ts-loader": "^9.2.8",
    "typescript": "^4.6.3",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2"
  }
}
